          "_name": "mobigarymenu1",
          "_sourceTheme": "mobirise5",
          "_customHTML": "<section class=\"menu\" group=\"Menu\" plugins=\"DropDown, TouchSwipe\" always-top global once=\"menu\" not-draggable position-absolute>\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\t<header>Global Menu Config</header>\n        <input type=\"range\" inline title=\"Menu Top Line Font Weight\" name=\"MenuTLFontWeight\" min=\"100\" max=\"900\" step=\"100\" value=\"400\">\n        <input type=\"range\" inline title=\"Menu Font Weight\" name=\"MenuFontWeight\" min=\"100\" max=\"900\" step=\"100\" value=\"400\">\n        <header>Menu Top Line (TL) Config</header>\n        <input type=\"checkbox\" title=\"Show TL\" name=\"showTopLine\" checked>\n        <input type=\"checkbox\" title=\"TL on HamMenu\" name=\"showTopLineHamMenu\" condition=\"showTopLine\">\n        <input type=\"checkbox\" title=\"TL on Scroll Sticky\" name=\"showTopLineOnScroll\" condition=\"showTopLine\">\n        <select title=\"TL Effect\" name=\"TopLineEffect\" condition=\"showTopLine\">\n            <option value=\"TopLineEffectNone\" selected>None</option>\n            <option value=\"TopLineEffectPulse\">Pulse</option>\n            <option value=\"TopLineEffectFlash\">Flash</option>\n            <option value=\"TopLineEffectScroll\">Scroll</option>\n        </select>\n        <input type=\"range\" inline title=\"TL Effect Pulse Speed\" name=\"TopLineEffectPulseSpeed\" min=\"0.5\" max=\"5\" step=\"0.1\" value=\"2.7\" condition=\"TopLineEffect=='TopLineEffectPulse' && showTopLine\">\n        <input type=\"range\" inline title=\"TL Effect Pulse Count\" name=\"TopLineEffectPulseCount\" min=\"1\" max=\"50\" step=\"1\" value=\"5\" condition=\"TopLineEffect=='TopLineEffectPulse' && showTopLine\">\n        <input type=\"range\" inline title=\"TL Effect Scroll Speed\" name=\"TopLineEffectScrollSpeed\" min=\"5\" max=\"100\" step=\"1\" value=\"15\" condition=\"TopLineEffect=='TopLineEffectScroll' && showTopLine\">\n        <input type=\"range\" inline title=\"TL Effect Scroll Window\" name=\"TopLineEffectScrollWindow\" min=\"50\" max=\"100\" step=\"0.5\" value=\"100\" condition=\"TopLineEffect=='TopLineEffectScroll' && showTopLine\">\n        <input type=\"range\" inline title=\"TL Effect Scroll Time Gap\" name=\"TopLineEffectScrollTimeGap\" min=\"1\" max=\"2\" step=\"0.1\" value=\"1\" condition=\"TopLineEffect=='TopLineEffectScroll' && showTopLine\">\n        <input type=\"range\" inline title=\"TL Effect Flash Speed\" name=\"TopLineEffectFlashSpeed\" min=\"0.5\" max=\"5\" step=\"0.1\" value=\"2.6\" condition=\"TopLineEffect=='TopLineEffectFlash' && showTopLine\">\n        <input type=\"range\" inline title=\"TL Effect Flash Count\" name=\"TopLineEffectFlashCount\" min=\"1\" max=\"50\" step=\"1\" value=\"5\" condition=\"TopLineEffect=='TopLineEffectFlash' && showTopLine\">\n        <input type=\"checkbox\" title=\"TL Scroll Mouse Hover Pause\" name=\"TopLineMouseHoverPause\" condition=\"TopLineEffect=='TopLineEffectScroll' && showTopLine\" checked>\n        <input type=\"range\" inline title=\"TL BG Opacity\" name=\"menuTopLineBgOpacity\" min=\"0\" max=\"1\" step=\"0.05\" value=\"1\" condition=\"showTopLine\">\n        <input type=\"color\" title=\"TL BG Color\" name=\"menuTopLineBgColor\" value=\"#275918\" condition=\"showTopLine\">\n        <input type=\"checkbox\" title=\"TL BG Gradient Color\" name=\"menuTopLineBgGradientCol\" condition=\"showTopLine\" checked>\n        <select title=\"TL BG Gradient Color Style\" name=\"menuTopLineGradientBgStyle\" condition=\"menuTopLineBgGradientCol && showTopLine\">\n            <option value=\"menuTopLineGradientBgLR2\">L-R 2 col</option>\n            <option value=\"menuTopLineGradientBgLR3\">L-R 3 Col</option>\n            <option value=\"menuTopLineGradientBgLR4\" selected>L-R 4 Col</option>\n            <option value=\"menuTopLineGradientBgTB2\">T-B 2 Col</option>\n            <option value=\"menuTopLineGradientBgTB3\">T-B 3 Col</option>\n            <option value=\"menuTopLineGradientBgTB4\">T-B 4 Col</option>\n        </select>\n        <input type=\"color\" title=\"TL BG Color 2\" name=\"menuTopLineBgColor2\" value=\"#f7ed4a\" condition=\"menuTopLineGradientBgStyle=='menuTopLineGradientBgLR2' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgLR3' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgLR4' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgTB2' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgTB3' && menuTopLineBgGradientCol && showTopLine  || menuTopLineGradientBgStyle=='menuTopLineGradientBgTB4'&&  menuTopLineBgGradientCol && showTopLine\">\n        <input type=\"color\" title=\"TL BG Color 3\" name=\"menuTopLineBgColor3\" value=\"#3bc444\" condition=\"menuTopLineGradientBgStyle=='menuTopLineGradientBgLR3' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgLR4' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgTB3' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgTB4' &&  menuTopLineBgGradientCol && showTopLine\">\n        <input type=\"color\" title=\"TL BG Color 4\" name=\"menuTopLineBgColor4\" value=\"#14f108\" condition=\"menuTopLineGradientBgStyle=='menuTopLineGradientBgLR4' && menuTopLineBgGradientCol && showTopLine || menuTopLineGradientBgStyle=='menuTopLineGradientBgTB4' && menuTopLineBgGradientCol && showTopLine\">\n        <input type=\"color\" title=\"TL Top Border Color\" name=\"TopLineULTopCol\" value=\"#232323\" condition=\"showTopLine\">\n        <input type=\"range\" inline title=\"TL Top Border Thickness\" name=\"TopLineULTopThicknessPX\" min=\"0\" max=\"3\" step=\"0.25\" value=\"0\" condition=\"showTopLine\">\n        <input type=\"color\" title=\"TL Bottom Border Color\" name=\"TopLineULBottomCol\" value=\"#232323\" condition=\"showTopLine\">\n        <input type=\"range\" inline title=\"TL Bottom Border Thickness\" name=\"TopLineULBottomThicknessPX\" min=\"0\" max=\"3\" step=\"0.25\" value=\"0\" condition=\"showTopLine\">\n        <input type=\"range\" inline title=\"TL Top Padding\" name=\"menuTopLinePaddingTop\" min=\"0\" max=\"25\" step=\"0.5\" value=\"2\" condition=\"showTopLine\">\n        <input type=\"range\" inline title=\"TL Bottom Padding\" name=\"menuTopLinePaddingBottom\" min=\"0\" max=\"25\" step=\"0.5\" value=\"2\" condition=\"showTopLine\">\n        <header>Main Menu (MM) Config</header>\n        <input type=\"range\" inline title=\"MM Max Width px\" name=\"MMMaxWidthPX\" min=\"1200\" max=\"1800\" step=\"20\" value=\"1600\">\n        <input type=\"checkbox\" title=\"MM Show Icons\" name=\"showIcons\" checked>\n        <input type=\"range\" inline title=\"MM Icons Area Width px\" name=\"MMIconsWidthAreaPX\" min=\"50\" max=\"250\" step=\"5\" value=\"75\" condition=\"showIcons\">\n        <select title=\"MM Icons\" name=\"iconsAmount\" condition=\"showIcons\">\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected>2</option>\n            <option value=\"3\">3</option>\n        </select>\n        <input type=\"checkbox\" title=\"MM Show Button(s)\" name=\"showButtons\" checked>\n        <input type=\"range\" inline title=\"MM Button Area Width px\" name=\"MMButtonWidthAreaPX\" min=\"150\" max=\"450\" step=\"5\" value=\"150\" condition=\"showButtons\">\n        <input type=\"checkbox\" title=\"MM Sticky\" name=\"sticky\" checked>\n        <input type=\"checkbox\" title=\"MM Collapsed\" name=\"collapsed\">\n        <input type=\"checkbox\" title=\"MM Show Logo\" name=\"showLogo\" checked>\n\t\t<input type=\"range\" title=\"MM Logo Top/Btm Padding px\" inline name=\"MMLogoPadding\" min=\"2\" max=\"10\" step=\"0.25\" value=\"6\" condition=\"showLogo\">\n        <input type=\"range\" title=\"MM Logo Size\" inline name=\"logoSize\" min=\"3.8\" max=\"8\" step=\"0.1\" value=\"3.8\" condition=\"showLogo\">\n        <input type=\"range\" title=\"MM Logo Size Scroll Sticky\" inline name=\"logoSizeOnScrollDown\" min=\"3.8\" max=\"8\" step=\"0.1\" value=\"3.8\" condition=\"showLogo\">\n        <input type=\"checkbox\" title=\"MM Show Brand Name\" name=\"showBrand\" checked>\n        <input type=\"checkbox\" title=\"MM Show Brand All screen px width\" name=\"showBrandAllScreenPXWidth\" condition=\"showBrand\">\n        <input type=\"range\" inline title=\"MM Brand Name Dont Show Under px\" name=\"MMBrandNameDontShowUnderPX\" min=\"250\" max=\"800\" step=\"5\" value=\"370\" condition=\"showBrand && !showBrandAllScreenPXWidth\">\n        <input type=\"range\" inline title=\"MM XS Brand Name Area Width px\" name=\"MMBrandNameWidthAreaXSpx\" min=\"100\" max=\"350\" step=\"5\" value=\"175\" condition=\"showBrand\">\n        <input type=\"range\" inline title=\"MM SM-LG Brand Name Area Width px\" name=\"MMBrandNameWidthAreaSMLGpx\" min=\"100\" max=\"500\" step=\"5\" value=\"310\" condition=\"showBrand\">\n        <input type=\"range\" inline title=\"MM LG-XL Brand Name Area Width px\" name=\"MMBrandNameWidthAreaLGXLpx\" min=\"100\" max=\"500\" step=\"5\" value=\"345\" condition=\"showBrand\">\n        <input type=\"range\" inline title=\"MM XL+ Brand Name Area Width px\" name=\"MMBrandNameWidthAreaXLpx\" min=\"100\" max=\"500\" step=\"5\" value=\"305\" condition=\"showBrand\">\n        <input type=\"checkbox\" title=\"MM Show Menu Items\" name=\"showItems\" checked>\n        <select title=\"MM Items Align\" name=\"menuAlign\" condition=\"showItems\">\n            <option value=\"left\">Left</option>\n            <option value=\"center\">Center</option>\n            <option value=\"right\" selected>Right</option>\n        </select>\n        <input type=\"range\" inline title=\"MM Item Spacing\" name=\"MainMenuItemSpacing\" min=\"1\" max=\"5\" step=\"0.1\" value=\"1.8\" condition=\"showItems\">\n        <input type=\"color\" title=\"MM BG Color\" name=\"menuBgColor\" value=\"#ffffff\">\n        <input type=\"checkbox\" title=\"MM Transparent\" name=\"transparent\">\n        <input type=\"range\" inline title=\"MM BG Opacity\" name=\"bgOpacity\" min=\"0.3\" max=\"1\" step=\"0.05\" value=\"0.85\" condition=\"!transparent\">\n        <input type=\"color\" title=\"MM Items Hover Color\" name=\"itemsHoverColor\" value=\"#232323\" condition=\"showItems\">\n        <input type=\"checkbox\" title=\"MM Hover Underline\" name=\"MainMenuHoverUnderline\" condition=\"showItems\" checked>\n        <input type=\"color\" title=\"MM Hov UL Color\" name=\"MMitemsHoverULCol\" value=\"#cc2952\" condition=\"MainMenuHoverUnderline && showItems\">\n        <input type=\"range\" inline title=\"MM Hover Underline Thickness\" name=\"MainMenuHoverUnderlineThickness\" min=\"1\" max=\"10\" step=\"0.25\" value=\"2\" condition=\"MainMenuHoverUnderline && showItems\">\n        <input type=\"checkbox\" title=\"MM Items Hov Bkg\" name=\"itemsHoverBkgChoice\" condition=\"showItems\" checked>\n        <input type=\"color\" title=\"MM Items Hov Bkg Color\" name=\"itemsHoverColBkg\" value=\"#cccccc\" condition=\"showItems && itemsHoverBkgChoice\">\n        <input type=\"color\" title=\"MM Underline Color\" name=\"menuLineColor\" value=\"#767676\">\n        <input type=\"range\" inline title=\"MM Underline Thickness\" name=\"MenuUnderlineThicknessPX\" min=\"0\" max=\"10\" step=\".25\" value=\"0.25\">\n        <input type=\"color\" title=\"Hamburger Color\" name=\"hamburgerColor\" value=\"#232323\">\n        <input type=\"color\" title=\"HamMenu L1 BG Color\" name=\"HamL1menuBgColor\" value=\"#ffffff\">\n        <input type=\"color\" title=\"HamMenu L2+ BG Color\" name=\"HamL2menuBgColor\" value=\"#efefef\">\n        <input type=\"range\" inline title=\"HamMenu BG Opacity\" name=\"HamMenuBgOpacity\" min=\"0.5\" max=\"1\" step=\"0.05\" value=\"0.95\">\n        <h